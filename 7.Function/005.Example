#include<iostream>
using namespace std;

int min(int a,int b)
{
    if(a<b)
    return a;
    else 
    return b;
}
int min(int a,int b,int c)
{
    if(a<b && a<c)
    return a;
    else if(b<c)
    return b;
    else 
    return c;
}

float min(float x,float y)
{
    if(x<y)
    return x;
    else 
    return y;
}
int main()
{
    cout<<min(10,5)<<endl;
    cout<<min(12,7,9)<<endl;
    cout<<min(18.0f,9.0f)<<endl;
}
